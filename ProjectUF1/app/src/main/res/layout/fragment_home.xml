<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeFragment">

    <FrameLayout android:id="@+id/frameHome"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylegray_500">
        <ImageView android:id="@+id/imageImagenOne"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            android:src="@drawable/img_imagen1"
            tools:ignore="ContentDescription" tools:src="@drawable/img_imagen1"/>
    </FrameLayout>
</FrameLayout>