<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimary">@color/your_color_primary</item>
        <item name="colorAccent">@color/your_color_accent</item>
        <item name="android:windowBackground">@color/your_color_primary</item>
        <item name="android:textColor">@color/your_color_accent</item>
    </style>
    <style name="etSolidRounded">
        <item name="android:textSize">18dp</item>
        <item name="android:textColor">#241c1c</item>
        <item name="android:textFontWeight" tools:targetApi="p">400</item>
        <item name="android:background">@drawable/rectangle1</item>
    </style>
    <style name="btnSolidRounded">
        <item name="android:textSize">18dp</item>
        <item name="android:textColor">#f5f4f2</item>
        <item name="android:textFontWeight" tools:targetApi="p">700</item>
        <item name="android:background">@drawable/rectangle_bg_indigo_400_radius_31_5</item>
    </style>
    <style name="btnSolidRounded_1">
        <item name="android:textSize">18dp</item>
        <item name="android:textColor">@color/gray_900</item>
        <item name="android:textFontWeight" tools:targetApi="p">400</item>
        <item name="android:background">@drawable/rectangle_bg_blue_400_b7_radius_31_5</item>
    </style>



</resources>